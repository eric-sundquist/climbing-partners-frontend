{
  "env": {
    "browser": true
  },
  "extends": [
      "airbnb",
      "prettier"
      ,
      // "eslint:recommended",
      // "plugin:react/recommended",
      "plugin:react/jsx-runtime"
  ],
  "plugins": [
      // "react",
      "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
  },
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-unused-vars": "warn",
    "no-console":"off",
    "no-process-exit":"off",
    "object-shorthand":"off",
    "jsx-a11y/anchor-is-valid": "off",
    "react/no-unescaped-entities": "off"
  }
}
